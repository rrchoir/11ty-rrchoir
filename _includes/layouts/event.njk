---
layout: layouts/base.njk
section: event
---

<article>
  <h1>{{ title }}</h1>

  {{ content | safe }}
   
  <h2>Song list</h2>
  {% for song in songs | sort(attribute='title') %}
    {% set songbits = song.split(":") %}
    <h4 style="font-size: 24px; font-weight: bold;">
      <a href="/songs/{{ songbits[1] | url }}">
        
        {{ songbits[0] }}
      </a>
    </h4>
  {% endfor %}
</article>

<nav>
  <a href="{{ '/events/' | url }}">Event index</a>
</nav>
